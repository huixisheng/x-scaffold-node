{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": [
        "types/*"
      ]
    },
    "typeRoots": [
      "types/**/*"
    ],
    "downlevelIteration": true,
    "lib": ["es2015"],
    "esModuleInterop": true
  }
}
